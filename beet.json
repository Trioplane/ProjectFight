{
    "name": "ProjectFight",
    "description": "WIP",
    "version": "v0.0.1",
    "author": "Trplnr",
    "require": ["bolt"],
    "data_pack": {
      "load": ["./src"],
      "pack_format": 81,
      "filter": {
        "block": [
          { "namespace": "minecraft", "path": "recipe/.*" },
          { "namespace": "minecraft", "path": "advancement/.*" }
        ]
      }
    },
    "resource_pack": {
      "load": ["./src"],
      "pack_format": 64
    },
    "pipeline": ["mecha", "beet.contrib.load"],
    "output": "./build",
    "meta": {
      "bolt": {"entrypoint": "projectfight:*"},
      "load": {
        "data_pack": "./libs/*"
      }
    }
}